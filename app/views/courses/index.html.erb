<h1>Courses</h1>
<% Course.all.each do |c|%>

  <h2><%=link_to c.title, course_path(c)%></h2>
  <h4><%=c.start_time%></h4>
  <h4>Age Group: <%=c.age_group%></h4>
  <p><%=c.description%></p>
<%end%>


<%= link_to "Go Back", parent_path(current_user)%>
<p><%= link_to "Log Out", logout_path, method: :delete %></p>
